<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TweensTalentScanner</name>
    <description>
        A scanner for the tweenstalent event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="phonegap-plugin-barcodescanner" spec="^8.0.0">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-advanced-http" spec="^1.11.1" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <platform name="android">
        <icon density="ldpi" src="./res/icon/android/android-icon-36x36.png" />
        <icon density="mdpi" src="./res/icon/android/android-icon-48x48.png" />
        <icon density="hdpi" src="./res/icon/android/android-icon-72x72.png" />
        <icon density="xhdpi" src="./res/icon/android/android-icon-96x96.png" />
        <icon density="xxhdpi" src="./res/icon/android/android-icon-144x144.png" />
        <icon density="xxxhdpi" src="./res/icon/android/android-icon-192x192.png" />
    </platform>
    <platform name="ios">
        <icon height="180" src="res/icon/ios/apple-icon-180x180.png" width="180" />
        <icon height="60" src="res/icon/ios/apple-icon-60x60.png" width="60" />
        <icon height="120" src="res/icon/ios/apple-icon-120x120.png" width="120" />
        <icon height="76" src="res/icon/ios/apple-icon-76x76.png" width="76" />
        <icon height="152" src="res/icon/ios/apple-icon-152x152.png" width="152" />
        <icon height="57" src="res/icon/ios/apple-icon-57x57.png" width="57" />
        <icon height="72" src="res/icon/ios/apple-icon-72x72.png" width="72" />
        <icon height="144" src="res/icon/ios/apple-icon-144x144.png" width="144" />
    </platform>
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <engine name="browser" spec="^5.0.3" />
    <engine name="android" spec="^7.0.0" />
    <engine name="ios" spec="^4.5.4" />
</widget>
